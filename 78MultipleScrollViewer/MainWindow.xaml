<Window x:Class="_78MultipleScrollViewer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_78MultipleScrollViewer"
        xmlns:uc="clr-namespace:_78MultipleScrollViewer.UserControls"
        mc:Ignorable="d"
        Name="MainWindowMetro"
        Title="MainWindow" Height="800" Width="1350">
    <Grid>
        <!--<uc:ProductivityTopApps></uc:ProductivityTopApps>-->
        <!--<uc:TopApps></uc:TopApps>-->
        <!--<uc:AppsViewer></uc:AppsViewer>-->
        <Frame Name="MainWindowFrame"
               Loaded="MainWindowFrame_Loaded"
               NavigationUIVisibility="Hidden"
               Width="{Binding ElementName=MainWindowMetro, Path=ActualWidth}"
               Height="{Binding ElementName=MainWindowMetro, Path=ActualHeight}">
                
        </Frame>
    </Grid>
</Window>
